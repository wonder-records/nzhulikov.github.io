{
  "locals": {
    "url": "http://wrecords.ru",
    "name": "Wonder Records",
    "owner": "Wonder Records",
    "description": "Студия звукозаписи в Новороссийске"
  },
  "output": "./www",
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-cname"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "moment": {
    "lang": "ru"
  },
  "paginator": {
    "articles": "articles",
    "template": "news.jade",
    "first": "news.html",
    "perPage": 5
  }
}
